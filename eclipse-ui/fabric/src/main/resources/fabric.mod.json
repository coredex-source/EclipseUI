{
  "schemaVersion": 1,
  "id": "eclipseui",
  "version": "${version}",
  "name": "EclipseUI",
  "description": "A modern, themeable configuration UI library for Minecraft mods",
  "authors": [
    "coredex-source"
  ],
  "contact": {
    "homepage": "https://github.com/coredex-source/EclipseUI",
    "sources": "https://github.com/coredex-source/EclipseUI",
    "issues": "https://github.com/coredex-source/EclipseUI/issues"
  },
  "license": "MIT",
  "icon": "assets/eclipseui/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "dev.eclipseui.fabric.EclipseUIFabric"
    ],
    "modmenu": [
      "dev.eclipseui.fabric.ModMenuIntegration"
    ]
  },
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "minecraft": ">=${minecraft_version} <${minecraft_version_max}",
    "java": ">=21",
    "eclipsecore": "*",
    "eclipseplatform": "*"
  },
  "suggests": {
    "modmenu": "*"
  },
  "custom": {
    "modmenu": {
      "badges": ["library"]
    }
  }
}
